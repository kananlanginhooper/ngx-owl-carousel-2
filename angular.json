{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "cli": {
        "analytics": "0ac52df0-769a-455f-bdf3-df53131f472e"
    },
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "ngx-owl-carousel2": {
            "projectType": "library",
            "root": "projects/ngx-owl-carousel2",
            "sourceRoot": "projects/ngx-owl-carousel2/src",
            "prefix": "lib",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:ng-packagr",
                    "options": {
                        "tsConfig": "projects/ngx-owl-carousel2/tsconfig.lib.json",
                        "project": "projects/ngx-owl-carousel2/ng-package.json"
                    },
                    "configurations": {
                        "production": {
                            "tsConfig": "projects/ngx-owl-carousel2/tsconfig.lib.prod.json"
                        }
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/ngx-owl-carousel2/src/test.ts",
                        "tsConfig": "projects/ngx-owl-carousel2/tsconfig.spec.json",
                        "karmaConfig": "projects/ngx-owl-carousel2/karma.conf.js"
                    }
                },
                "lint": {
                    "builder": "@angular-eslint/builder:lint",
                    "options": {
                        "eslintConfig": ".eslintrc.json",
                        "lintFilePatterns": ["**/*.ts", "**/*.html"]
                    },
                    "exclude": ["**/node_modules/**", "**/dist/**"]
                }
            }
        }
    },
    "defaultProject": "ngx-owl-carousel2"
}
